[user]
	name = Leandro Gualter
	email = leandrogualter@gmail.com
[branch]
  autosetuprebase = always
[apply]
  whitespace = warn
[alias]
  co = checkout
  ci = commit
  st = status
  br = branch
  type = cat-file -t
  dump = cat-file -p
  ll = log --graph --decorate --pretty=oneline --abbrev-commit
  pr = pull -r
[push]
	default = current
[core]
	excludesfile = /Users/spare/.gitignore_global
[difftool "sourcetree"]
	cmd = opendiff \"$LOCAL\" \"$REMOTE\"
	path = 
[mergetool "sourcetree"]
	cmd = /Applications/SourceTree.app/Contents/Resources/opendiff-w.sh \"$LOCAL\" \"$REMOTE\" -ancestor \"$BASE\" -merge \"$MERGED\"
	trustExitCode = true
